{
	"name": "express-abstract-sessions",
	"version": "1.0.0",
	"description": "A TS-based backend skeleton for web3 authentication and session management",
	"type": "commonjs",
	"main": "dist/main.js",
	"scripts": {
		"build": "tsc",
		"start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
		"start:prod": "node dist/main.js",
		"lint": "eslint './src/**/*.ts' --ext .ts",
		"lint:fix": "eslint './src/**/*.ts' --fix",
		"format": "prettier --write 'src/**/*.{js,ts,json}'"
	},
	"dependencies": {
		"@abstract-foundation/agw-client": "^1.7.1",
		"@prisma/client": "^6.7.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"prisma": "^6.7.0",
		"viem": "^2.29.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.26.0",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.18",
		"@types/express": "^5.0.1",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/node": "^22.15.18",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"eslint": "^9.26.0",
		"prettier": "^3.5.3",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.8.3"
	}
}
